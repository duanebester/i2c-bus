{
	"name": "i2c-bus",
	"version": "5.2.3",
	"description": "I2C serial bus access with Node.js",
	"main": "i2c-bus.js",
	"directories": {
		"example": "example",
		"test": "test"
	},
	"scripts": {
		"coverage-report": "nyc report --reporter=html",
		"install": "node-gyp rebuild",
		"integration-test": "cd integration-test && ./run-tests && cd ..",
		"lint": "jshint i2c-bus.js example/*.js integration-test/*.js test/*.js test/*/*.js",
		"test": "nyc mocha"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fivdi/i2c-bus.git"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"nan": "^2.22.2"
	},
	"devDependencies": {
		"async": "^3.2.6",
		"glob": "^11.0.2",
		"jshint": "^2.13.6",
		"lodash": "^4.17.21",
		"mocha": "^11.5.0",
		"mock-fs": "^5.5.0",
		"mock-require": "^3.0.3",
		"nyc": "^17.1.0",
		"sinon": "^20.0.0"
	},
	"keywords": [
		"i2c",
		"iot",
		"raspberry",
		"raspi",
		"rpi",
		"pi",
		"beaglebone",
		"smbus",
		"linux"
	],
	"author": "fivdi",
	"license": "MIT",
	"gypfile": true
}
